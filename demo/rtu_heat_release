export SOURCE=demo/test-deploy
export CONFIG=demo/modbus_configs/m2000_configs/m2000_htg_release.config 
export TAG=release_htg

./scripts/core/make-agent.sh 
sleep 1
volttron-ctl stop --tag release_htg
volttron-ctl remove --tag release_htg
volttron-ctl clear
volttron-ctl status
# To set the agent to autostart with the platform, pass "enable" 
# to make-agent.sh: ./scripts/core/make-agent.sh enable
