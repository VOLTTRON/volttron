# The following need to be modified to customize for your specific deployment

# The directory where the agent is stored.  Note the current assumes that you
# are executing this script from the root volttron directory.
export SOURCE=examples/ListenerAgent/ 
export CONFIG=examples/ListenerAgent/config 
export TAG=listener 

./scripts/core/make-agent.sh 

# To set the agent to autostart with the platform, pass "enable" 
# to make-agent.sh: ./scripts/core/make-agent.sh enable
